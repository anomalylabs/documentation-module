{
    "name": "anomaly/documentation-module",
    "type": "streams-addon",
    "description": "A powerful commenting system.",
    "keywords": [
        "streams module",
        "streams addon",
        "comments"
    ],
    "homepage": "http://pyrocms.com/",
    "license": "MIT",
    "authors": [{
            "name": "PyroCMS, Inc.",
            "homepage": "http://pyrocms.com/",
            "role": "Owner"
        },
        {
            "name": "Ryan Thompson",
            "homepage": "http://ryanthepyro.com/",
            "role": "Developer"
        }
    ],
    "support": {
        "docs": "https://pyrocms.com/documentation/documentation-module",
        "streams": "https://pyrocms.com/forum/channels/documentation-module",
        "slack": "https://pyrocms.com/slack",
        "issues": "https://github.com/pyrocms/pyrocms/issues?q=is:issue is:open [documentation-module]",
        "source": "https://github.com/anomalylabs/documentation-module"
    },
    "require": {
        "anomaly/streams-platform": "^2.0"
    },
    "suggest": {
        "anomaly/local_documentation-extension": "Store documentation source locally.",
        "anomaly/github_documentation-extension": "Store documentation source in a GitHub repository."
    },
    "autoload": {
        "psr-4": {
            "Anomaly\\DocumentationModule\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anomaly\\DocumentationModule\\DocumentationModuleServiceProvider"
            ]
        }
    }
}
